s16 f(s16 level) {
    s16 a;
    s16 b;
    s16 r;
    if (level != 0) {
        a = 0xa55a + level;
        b = 0x7531 + level;
        r = f(level - 1);
        ASSERT_TRUE(a == 0xa55a + level);
        ASSERT_TRUE(b == 0x7531 + level);
        ASSERT_TRUE(r == level + 6);
    } else {
        a = 1000;
        b = 1000;
    }
    return level + 7;
}

